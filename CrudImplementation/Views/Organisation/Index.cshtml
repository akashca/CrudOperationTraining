@model IEnumerable<CrudImplementation.Models.Organisation>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
   
        <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.OrganisationId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OrganisationName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OrganisationAddress)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.OrganisationId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.OrganisationName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.OrganisationAddress)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.OrganisationId }) |
                    @Html.ActionLink("Details", "Details", new { id = item.OrganisationId }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.OrganisationId })
                </td>
            </tr>
        }

    </tbody>
</table>
  